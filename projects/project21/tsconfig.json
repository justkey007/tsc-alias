{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es5",
    "module": "commonjs",
    "strictNullChecks": false,
    "noImplicitAny": false,
    "baseUrl": "./src",
    "paths": {
      "$constants": ["constants"]
    },
    "moduleResolution": "node"
  },
  "include": ["src/**/*.ts", "test/**/*.ts"],
  "exclude": ["node_modules"],
  "tsc-alias": {}
}
